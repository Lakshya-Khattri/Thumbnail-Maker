{
  "name": "thumbnail-forge",
  "version": "1.0.0",
  "description": "AI-powered YouTube thumbnail generator using Claude API and HTML5 Canvas",
  "main": "server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && react-scripts start",
    "build": "cd client && react-scripts build",
    "start": "node server/server.js",
    "install:all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": ["youtube", "thumbnail", "ai", "claude", "canvas"],
  "author": "Your Name",
  "license": "MIT"
}
